data $arr = { l 0 0 0 0 0 }

export function l $main()
{
@start

    %i =l alloc8 8
    storel 0, %i
    %x =l alloc8 8
    storel 0, %x
    %y =l alloc8 8
    storel 0, %y
    storel 2, %i
    storel 10, %x
    storel 20, %y
    %y_1 =l loadl %i
    %y_2 =l loadl %x
    %y_3 =l loadl %y
    %y_4 =l add %y_2, %y_3
    %y_5 =l add $arr, %y_1
    storel %y_4, %y_5

    ret 0
}


