# FunctionDecleration
export function l $main()
{
@start

    # ExternalStatement
    # AutoStatement
    # auto a = 0
    %a_0 =l alloc8 8
    storew 0, %a_0
    # auto b = 0
    %b_0 =l alloc8 8
    storew 0, %b_0
    # auto c = 0
    %c_0 =l alloc8 8
    storew 0, %c_0
    # VariableAssignment a = 12
    storel 12, %a_0
    # FunctionCall
    # call printf
    %a_1 =l loadl %a_0
    call $printf(l $string_0, l %a_1)
    # VariableAssignment b = &a
    # VisitAddressOfExpression: &a
    storel %a_2, %b_0
    # VariableAssignment c = *b
    # VisitAddressOfExpression: *b
    # Load Pointer
    %b_2 =l loadl %b_1
    # Dereferece Pointer
    %b_3 =l loadl %b_2
    storel %b_3, %c_0
    # FunctionCall
    # call printf
    %c_1 =l loadl %c_0
    call $printf(l $string_1, l %c_1)

    ret 0
}

# Data
data $string_0 = { b "a %d\n", b 0 }
data $string_1 = { b "c %d\n", b 0 }

