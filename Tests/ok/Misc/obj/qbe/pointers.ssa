export function w $main()
{
@start
    %a_0 =l alloc8 8

    # auto a = 0
    storew 0, %a_0
    %b_1 =l alloc8 8

    # auto b = 0
    storew 0, %b_1
    %c_2 =l alloc8 8

    # auto c = 0
    storew 0, %c_2

    # a = 12
    storew 12, %a_0

    # call printf
    %a_3 =w loadw %a_0
    call $printf(l $string_0, w %a_3)

    # b = &a
    storew %a_1, %b_0

    # c = *b
    %b_4 =l loadw %b_0
    %b_5 =w loadw %b_4
    storew %b_5, %c_0

    # call printf
    %c_6 =w loadw %c_0
    call $printf(l $string_1, w %c_6)
    ret 0
}


# Data
data $string_0 = { b "a %d\n", b 0 }
data $string_1 = { b "c %d\n", b 0 }


