
export function l $main()
{
@start

    %x =l alloc8 8
    storel 0, %x
    %y =l alloc8 8
    storel 0, %y
    storel 5, %x
    storel 3, %y
    %y_1 =l loadl %x
    %y_2 =l loadl %y
    %y_3 =l add %y_1, %y_2
    call $printf(l $string_0, l %y_3)

    ret 0
}

data $string_0 = { b "%d\n", b 0 }

