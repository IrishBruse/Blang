    # y = 3
    storew 3, %y_0

    # call printf

    # (x + y)
    %x_2 =w loadw %x_0
    %y_3 =w loadw %y_0
    %y_4 =w add %x_2, %y_3
    call $printf(%y_4)
    ret 0
}


