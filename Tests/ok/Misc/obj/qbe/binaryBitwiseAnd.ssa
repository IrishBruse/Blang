# FunctionDecleration
export function l $main()
{
@start

    # ExternalStatement
    # AutoStatement
    # auto result = 0
    %result_0 =l alloc8 8
    storew 0, %result_0
    # VariableAssignment result = (5 & 3)
    # VisitBinaryExpression: &
    storel %BitwiseAnd, %result_0
    # FunctionCall
    # call printf
    %result_1 =l loadl %result_0
    call $printf(l $string_0, l %result_1)

    ret 0
}

# Data
data $string_0 = { b "%d\n", b 0 }

