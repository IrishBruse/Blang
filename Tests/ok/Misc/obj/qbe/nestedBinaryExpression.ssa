
export function l $main()
{
@start

    %result =l alloc8 8
    storel 0, %result
    %a =l alloc8 8
    storel 0, %a
    %b =l alloc8 8
    storel 0, %b
    %c =l alloc8 8
    storel 0, %c
    %d =l alloc8 8
    storel 0, %d
    storel 5, %a
    storel 3, %b
    storel 2, %c
    storel 1, %d
    %result_1 =l loadl %a
    %result_2 =l loadl %b
    %result_3 =l add %result_1, %result_2
    %result_4 =l loadl %c
    %result_5 =l loadl %d
    %result_6 =l sub %result_4, %result_5
    %result_7 =l mul %result_3, %result_6
    storel %result_7, %result
    %result_8 =l loadl %result
    call $printf(l $string_0, l %result_8)

    ret 0
}

data $string_0 = { b "%d\n", b 0 }

