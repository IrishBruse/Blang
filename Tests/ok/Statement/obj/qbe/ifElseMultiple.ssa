export function l $main()
{
@start

    %x_0 =l alloc8 8
    storew 0, %x_0
    storel 2, %x_0
    @if_1_condition
        %x_1 =l loadl %x_0
        %x_2 =l ceql %x_1, 1
        jnz %x_2, @if_1_body, @if_1_else
    @if_1_body
        call $printf(l $string_0)
        jmp @if_1_end
    @if_1_else
        @if_2_condition
            %x_3 =l loadl %x_0
            %x_4 =l ceql %x_3, 2
            jnz %x_4, @if_2_body, @if_2_else
        @if_2_body
            call $printf(l $string_1)
            jmp @if_2_end
        @if_2_else
            call $printf(l $string_2)
        @if_2_end
    @if_1_end

    ret 0
}

data $string_0 = { b "one\n", b 0 }
