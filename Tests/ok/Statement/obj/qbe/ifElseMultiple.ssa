
export function l $main()
{
@start

    %x =l alloc8 8
    storel 0, %x
    storel 2, %x
    @if_1_condition
        %if_1 =l loadl %x
        %if_2 =l ceql %if_1, 1
        jnz %if_2, @if_1_body, @if_1_else
    @if_1_body
        call $printf(l $string_0)
        jmp @if_1_end
    @if_1_else
        @if_2_condition
            %if_3 =l loadl %x
            %if_4 =l ceql %if_3, 2
            jnz %if_4, @if_2_body, @if_2_else
        @if_2_body
            call $printf(l $string_1)
            jmp @if_2_end
        @if_2_else
            call $printf(l $string_2)
        @if_2_end
    @if_1_end

    ret 0
}

data $string_0 = { b "one\n", b 0 }
data $string_1 = { b "two\n", b 0 }
data $string_2 = { b "other\n", b 0 }

