
export function l $main()
{
@start

    @if_1_condition
        %if_1 =l csltl 1, 0
        jnz %if_1, @if_1_body, @if_1_else
    @if_1_body
        call $printf(l $string_0)
        jmp @if_1_end
    @if_1_else
        call $printf(l $string_1)
    @if_1_end

    ret 0
}

data $string_0 = { b "Fail\n", b 0 }
data $string_1 = { b "Pass\n", b 0 }

